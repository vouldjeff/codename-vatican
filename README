Here is a demo of Entity.properties format:

{
  "/people/actor"=>{ 
    "name"=>"Actor", 
    "type_properties"=>{
      "/people/actor/played_in"=>[
        {
          "label"=>"Play", 
          "value"=>"The swan princes"
        }, 
        {
          "label"=>"Play", 
          "value"=>"Arthur"
        }, 
        {
          "label"=>"Play", 
          "value"=>"Rapunzel"
        }
      ]
    }
  }, 
  "/people/person"=>{
    "name"=>"Person", 
    "type_properties"=>{
      "/people/person/mother"=>{
        "label"=>"Mother", 
        "value"=>"Angelina"
      }, 
      "/people/person/father"=>{
        "label"=>"Father", 
        "value"=>"Marin"
      }, 
      "/people/person/birth_place"=>{
        "label"=>"Birth place", 
        "value"=>"Sofia"
      }
    }
  }
}
