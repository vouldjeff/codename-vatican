Here is a demo of Entity.properties format:

{
  "/people/actor"=>{ 
    "name"=>"Actor", 
    "type_properties"=>[
      {
        "key"=>"/people/actor/played_in",
        "label"=>"Play",
        "value"=>[
          {
            "text"=>"The swan princes"
          },
          {
            "text"=>"Rapunzel"
          },
          {
            "text"=>"Arthur"
          }
        ]
      }
    ]
  }, 
  "/people/person"=>{
    "name"=>"Person", 
    "type_properties"=>[
      {
        "key"=>"/people/person/mother",
        "label"=>"Mother", 
        "value"=>{
          "text"=>"Angelina"
        }
      }, 
      {
        "key"=>"/people/person/father",
        "label"=>"Father", 
        "value"=>{
          "text"=>"Marin",
        }
      }, 
      {
        "key"=>"/people/person/birth_place",
        "label"=>"Birth place", 
        "value"=>{
          "text"=>"Sofia",
        }
      }
    ]
  }
}
