<% title "Типове" %>
<%= link_to 'назад', "/backend", :class => "back" %>

<table class="sorted-table">
  <thead>
    <tr>
      <th>Име</th>
      <th width="100px"></th>
    </tr>
  </thead>
  <tbody>
  <% @types.each do |type| %>
    <tr>
      <td><%= type.name %></td>
      <td><%= link_to "<span>Промени</span>".html_safe, edit_backend_type_path(:id => type._id), :class => "icon-link icon-sprite-edit" %></td>
    </tr>
  <% end %>
  </tbody>
</table>
